{
  "name": "grocery-list",
  "version": "0.1.0",
  "description": "Just a basic app for managing a household's shared grocery list",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robinmaypanpan/grocerylist"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "grocery"
  ],
  "scripts": {
    "start-server": "node server/index.js",
    "test-server": "node server/test.js",
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "test-ui": "react-scripts test --testPathIgnorePatterns=server/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
